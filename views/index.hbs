<h1>{{title}}</h1>

<table class="table table-striped table-hover">
    <tr>
        <th>Course</th>
        <th>Assignment</th>
        <th>Due</th>
    </tr>
    {{#each upcoming}}
        <tr>
            <td>{{ this.course }}</td>
            <td>{{ this.work }}</td>
            <td>{{ this.due }} </td>/td>
            <td>
                <a href="{{this.link}}">Go to</a>
            </td>
        </tr>
    {{/each}}
</table>
